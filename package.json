{
  "name": "@wiidede/dan-player",
  "type": "module",
  "version": "0.1.1",
  "packageManager": "pnpm@9.13.2",
  "description": "A player supporting danmaku using Comment Core Library",
  "author": "wiidede <wiixdede@gmail.com>",
  "license": "MIT",
  "funding": "https://wiidede.space/sponsor",
  "homepage": "https://github.com/wiidede/dan-player#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wiidede/dan-player.git"
  },
  "bugs": "https://github.com/wiidede/dan-player/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./style.css": "./dist/style.css",
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite playground --mode play",
    "dev:pkg": "vite playground",
    "preview": "vite preview playground",
    "build:play": "vite build && vite build playground",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "pnpm lint && pnpm typecheck && pnpm build && bumpp",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@vueuse/core": "^11.2.0",
    "@wiidede/comment-core-library": "^0.2.0",
    "element-plus": "^2.8.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.9.1",
    "@antfu/ni": "^0.23.0",
    "@antfu/utils": "^0.7.10",
    "@iconify-json/carbon": "^1.2.4",
    "@iconify/utils": "^2.1.33",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.9.0",
    "@unocss/eslint-config": "^0.64.1",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vue/test-utils": "^2.4.6",
    "bumpp": "^9.8.1",
    "defu": "^6.1.4",
    "eslint": "^9.15.0",
    "eslint-plugin-format": "^0.1.2",
    "esno": "^4.8.0",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.13.2",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.6.3",
    "unocss": "^0.64.1",
    "unplugin-auto-import": "^0.18.4",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-inspect": "^0.8.7",
    "vitest": "^2.1.5",
    "vue": "^3.5.13",
    "vue-tsc": "^2.1.10"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
